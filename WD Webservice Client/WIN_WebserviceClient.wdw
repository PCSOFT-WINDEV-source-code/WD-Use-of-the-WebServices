   -
     name : DisplayResult
     procedure_id : 1246122806041776098
     type_code : 14
     code : |1+
      Procedure DisplayResult()
      
      // Display the image
      Image_Result = fExeDir()+“\Temp.jpg”
     type : 458752
   -
     name : bRetrieveImage
     procedure_id : 1246122806041841634
     type_code : 14
     code : |1-
      Procedure bRetrieveImage(nImageNum)
      
      sResult is string
      nFileID is int
      
      
      // Call to the Web service
      sResult =  Uncrypt(WD_Serveur_Webservice.ReturnImage(nImageNum), “”,cryptNone,encodeBASE64)
      
      IF sResult~=“” THEN
      	Error(<§$0008§>,ErrorInfo())
      	RESULT False
      END
      
      // Create the viewing file 
      nFileID = fOpen(fExeDir()+“\Temp.jpg”,foCreate)
      IF nFileID<>-1 THEN
      	// Rebuild the file
      	fWrite(nFileID,sResult,Length(sResult))
      	IF ErrorOccurred THEN
      		Error(<§$0009§>,ErrorInfo())
      		RESULT False
      	END
      	fClose(nFileID)
      	
      	// Display the image
      	DisplayResult()
      ELSE
      	Error(<§$000a§>,ErrorInfo())
      	RESULT False
      END
      
      // By default, the retrieval is successfully performed
      RESULT True
     type : 458752
   -
     name : LastResult
     procedure_id : 1246122806041907170
     type_code : 14
     code : |1-
      Procedure LastResult()
      
      // Search for the first result
      gfnResultSubscript = Dimension(gfnResultsTable)
      
      // Check the dimension of the result array
      IF Dimension(gfnResultsTable)<gfnResultSubscript OR gfnResultSubscript =0 THEN
      	Info(<§$000b§>)
      	RETURN
      END
      
      // Search for the corresponding image
      IF bRetrieveImage(gfnResultsTable[gfnResultSubscript])=False THEN
      	// Error retrieving the image
      	Info(<§$000c§>)
      	RETURN
      END
      
      // Display the result
      DisplayResult()
     type : 458752
   -
     name : RunSearch
     procedure_id : 1246122806041972706
     type_code : 14
     code : |1+
      Procedure RunSearch(bSearchOR)
      
      arrSearchResult is ImageSearchResponse
      arrRes is array of string
      nNbResults is int
      nOccurrence is int
      sKeywords is string
      
      // Call to the Web service
      nOccurrence = ListCount(LIST_KeywordsToFind)
      FOR i= 1 TO nOccurrence
      	sKeywords += [“$”] + LIST_KeywordsToFind[i]
      END
      
      arrSearchResult = WD_Serveur_Webservice.ImageSearch(sKeywords,bSearchOR)
      
      // Process the result
      IF arrSearchResult.ImageSearchResult..Occurrence > 0 THEN
      	
      	FOR i = 1 _TO_ arrSearchResult.ImageSearchResult..Occurrence
      		ArrayAdd(arrRes,arrSearchResult.ImageSearchResult[i])
      	END
      	
      	// Store the result
      	nNbResults = nStoreResult(arrRes)
      	
      	// Display the number of result images
      	SWITCH nNbResults
      		CASE 0 :
      			STC_Result= <§$000f§>
      			Image_Result = “”
      			
      		CASE 1 :
      			STC_Result= <§$0010§>
      		OTHER CASE :
      			STC_Result= StringBuild(<§$000d§>, nNbResults)
      	END
      	
      	
      	// Display the corresponding image
      	FirstResult()
      	
      	// Display the browse buttons if there is more than one result
      	IF nNbResults>=1 THEN
      		GROUP_BROWSE..Visible = True
      	ELSE
      		GROUP_BROWSE..Visible = False
      	END
      ELSE
      	// Display the error
      	STC_Result= <§$000f§>
      	Image_Result = “”
      	
      	Error(<§$000e§>)
      END
     type : 458752
   -
     name : nStoreResult
     procedure_id : 1246122806042038242
     type_code : 14
     code : |1+
      Procedure nStoreResult(arrSearchResult)
      
      nNbReponses is int
      
      // Free the former array of results
      gfnResultsTable = new array of int
      
      // Define the number of answers 
      nNbReponses = ArrayCount(arrSearchResult)
      
      // Resize the array to the requested size
      gfnResultsTable = arrSearchResult
      
      // Return the number of responses
      RESULT nNbReponses
      
     type : 458752
   -
     name : FirstResult
     procedure_id : 1246122806042103778
     type_code : 14
     code : |1-
      Procedure FirstResult()
      
      // Search for the first result
      gfnResultSubscript = 1
      
      // Check the dimension of the result array
      IF Dimension(gfnResultsTable)<gfnResultSubscript THEN
      	Info(<§$000b§>)
      	RETURN
      END
      
      // Search for the corresponding image
      IF bRetrieveImage(gfnResultsTable[gfnResultSubscript])=False THEN
      	// Error retrieving the image
      	Info(<§$000c§>)
      	RETURN
      END
      
      // Display the result
      DisplayResult()
     type : 458752
   -
     name : PreviousResult
     procedure_id : 1246122806042169314
     type_code : 14
     code : |1-
      Procedure PreviousResult()
      
      IF gfnResultSubscript <= 1 THEN
      	Info(<§$0011§>)
      	FirstResult()
      	RETURN
      END
      
      // Search for the first result
      gfnResultSubscript --
      
      // Check the dimension of the result array
      IF Dimension(gfnResultsTable)<gfnResultSubscript THEN
      	Info(<§$000b§>)
      	RETURN
      END
      
      // Search for the corresponding image
      IF bRetrieveImage(gfnResultsTable[gfnResultSubscript])=False THEN
      	// Error retrieving the image
      	Info(<§$000c§>)
      	RETURN
      END
      
      // Display the result
      DisplayResult()
     type : 458752
   -
     name : NextResult
     procedure_id : 1246122806042234850
     type_code : 14
     code : |1-
      Procedure NextResult()
      
      // Search for next result
      gfnResultSubscript++
      
      // Check the dimension of the result array
      IF Dimension(gfnResultsTable)<gfnResultSubscript THEN	
      	IF Dimension(gfnResultsTable) <>0 THEN 
      		Info(<§$0012§>)
      		LastResult()	
      	ELSE
      		Info(<§$000b§>)	
      	END
      	RETURN
      	
      END
      
      // Search for the corresponding image
      IF bRetrieveImage(gfnResultsTable[gfnResultSubscript])=False THEN
      	// Error retrieving the image
      	Info(<§$000c§>)
      	RETURN
      END
      
      // Display the result
      DisplayResult()
     type : 458752
