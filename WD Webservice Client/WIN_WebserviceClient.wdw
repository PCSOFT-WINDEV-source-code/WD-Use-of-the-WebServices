      CONSTANT CST_WDL_NAME = “WD Webservice Server”
      
      GLOBAL
      	gfnResultsTable is array dynamic		// Array containing the result of the search
      	gfnResultSubscript is int					// Current subscript in the search result
      	gfsINIFile is string
      LOCAL
      	
      gfnResultSubscript = 1
      
      // Configuration ".ini" file
      gfsINIFile = fExeDir()+“\”+ProjectInfo(piProjectName)+“.ini”
      IF NOT fFileExist(gfsINIFile) THEN
      	fCreate(gfsINIFile)
      END
      
      GROUP_BROWSE..Visible = False
